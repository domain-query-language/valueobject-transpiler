{
  "name": "eventsourced/valueobjects-transpiler",
  "description": "A code Generator for ValueObjects",
  "license": "MIT",
  "authors": [
    {
      "name": "Colin Lyons",
      "email": "colin@craftingdomains.com"
    },
    {
      "name": "Barry O'Sullivan",
      "email": "barry@craftingdomains.com"
    },
    {
      "name": "Kiran Guragai",
      "email": "kiran@craftingdomains.com"
    }
  ],
  "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/barryosull/valueobjects"
      }
    ],
  "require": {
    "eventsourced/valueobjects": "dev-master",
    "league/flysystem-memory": "^1.0",
    "league/plates": "^3.1",
    "symfony/yaml": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.2.1"
  },
  
  "autoload": {
    "psr-4": {
      "Adapter\\": "src/Adapter/",
      "App\\": "src/App/",
      "Domain\\": "src/Domain/",
      "Controller\\": "src/Controller",
      "Test\\": "test/"
    }
  },
  
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  
  "minimum-stability": "dev"
}
